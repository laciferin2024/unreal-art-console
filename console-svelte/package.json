{
  "name": "unreal-console",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "private": true,
  "description": "Unreal AI: The Open, On-Chain AI API for Builders",
  "author": "Unreal AI Team",
  "type": "module",
  "scripts": {
    "start": "vite preview --outDir build/ --port 8080",
    "start:py": "python3 -m http.server -d build/ 8080",
    "start:gulp": "gulp start",
    "setup:min": "npm install -g bun && mkdir -p tests/.data",
    "postsetup:min": "bun install && bun check",
    "setup": "npm run setup:min",
    "postsetup": "bunx -y playwright install --with-deps",
    "style": "prettier -w .",
    "prebuild": "bun check && rimraf build ",
    "b": "bun run build",
    "build": "vite build",
    "postbuild": "bun sync && bun inline0 && rimraf build/_* ",
    "dev": "vite dev",
    "preview": "vite preview",
    "format": "prettier --plugin-search-dir . --write .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "netlify:dev": "netlify dev",
    "predeploy:netlify": "netlify build",
    "d": "bun run deploy",
    "deploy": "bun run deploy:netlify",
    "deploy:prod": "bun run deploy:netlify --prod",
    "deploy:netlify": "netlify deploy",
    "dev:functions": "NODE_OPTIONS=--inspect netlify functions:serve --functions functions --port 8081",
    "clean": "rimraf .svelte-kit build",
    "clean:t": "rimraf tests/.data/*",
    "inline0": "bun removeInlineScript.cjs",
    "inline": "bun scripts/inline.ts",
    "inline2": "bun scripts/inline2.ts",
    "sync": "bun scripts/sync-csp.ts && cp static/manifest.json build/",
    "shad": "bunx shadcn-svelte ",
    "shad:add": "bun shad add ",
    "gen:pkg": "svelte-kit package",
    "prerelease": "bun git:changelist",
    "release": "bun scripts/release.ts",
    "rel": "bun release -t=",
    "postrel": "bun b",
    "git:changelist": "sh scripts/changelist.sh",
    "git": "bun git:changelist",
    "prezip:build": "bun run build",
    "zip:build": "zip -r build.zip build",
    "zip": "zip -r build.zip build",
    "cy": "bun run cypress open",
    "t": "bun run test",
    "test": "playwright test",
    "test-report": "playwright show-report",
    "test:gen": "playwright codegen localhost:5173",
    "test:prod": "bun start & CI=1 PORT=8080 bun run test",
    "test:preview": "PORT=4173 CI=1 bun run test",
    "prod": "bun run build && bun run start"
  },
